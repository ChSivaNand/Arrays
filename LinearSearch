package Algorithms;

public class LinearSearch {

	public static void main(String[] args) {
		
		int[] a= {2,5,6,8,1,0};
		int index=-1;
		int ele=1;
		for(int i=0;i<a.length;i++) {
			if(a[i]==ele) {
				index=i;
			}
		}
		if(index==-1) {
			System.out.println("Element Not Found...!");
		}
		else {
			System.out.println("Element Found In "+index+" Position....!");
		}
	}
}
